<h1>配送先編集</h1>

<div>
<%= form_with model: @shopping_address, local: true do |f| %>

  <label>郵便番号</label>
  <%= f.text_field :postcode %>
  
  <label>住所</label>
  <%= f.text_field :address, size: 55 %>
  
  <label>宛名</label>
  <%= f.text_field :name %>
  
  <%= f.submit '変更内容を保存', class: "btn btn-success" %>

<% end %>
</div>